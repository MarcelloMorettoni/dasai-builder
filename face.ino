const int IDLE_ACTION_COUNT =
  sizeof(idleActions) / sizeof(idleActions[0]);

// =====================================================
// RENDER HELPERS
// =====================================================
void draw(const unsigned char* bmp) {
  u8g2.clearBuffer();
  u8g2.drawXBMP(0, 0, 128, 64, bmp);
  u8g2.sendBuffer();
}

// Play animation ONCE and stop on last frame
void playOnce(const Animation& a) {
  for (int i = 0; i < a.count; i++) {
    draw(a.frames[i]);
    delay(a.delayMs);
  }
}

// Draw last frame instantly
void showStatic(const Animation& a) {
  draw(a.frames[a.count - 1]);
}

// Hold last frame for visible pause
void holdLastFrame(const Animation& a, unsigned long ms) {
  draw(a.frames[a.count - 1]);
  unsigned long start = millis();

  while (millis() - start < ms) {
    handleButton();
    if (stateJustChanged) return;
  }
}

// =====================================================
// IDLE ACTION PLAYER
// =====================================================
void playIdleAction(IdleAction action) {

  // Start from center
  showStatic(anim_center);

  switch (action) {

    case IDLE_BLINK:
      playOnce(anim_blink);
      break;

    case IDLE_LOOK_LEFT:
      playOnce(anim_look_left);
      delay(1500);
      holdLastFrame(anim_look_left, 2200);
      break;

    case IDLE_LOOK_RIGHT:
      playOnce(anim_look_right);
      delay(1500);
      holdLastFrame(anim_look_right, 2200);
      break;

    case IDLE_LOOK_UP:
      playOnce(anim_look_up);
      delay(500);
      holdLastFrame(anim_look_up, 2600);
      break;
  }

  // Return to center and settle
  showStatic(anim_center);
  delay(300);
}

// =====================================================
// IDLE LOOP (ONLY ONE  DO NOT DUPLICATE)
// =====================================================
void idle() {
  static IdleAction lastAction = IDLE_BLINK;

  IdleAction action;
  do {
    action = idleActions[random(IDLE_ACTION_COUNT)];
  } while (action == lastAction);

  lastAction = action;
  playIdleAction(action);
}

// =====================================================
// BUTTON HANDLER (RELIABLE, EDGE-TRIGGERED)
// =====================================================
void handleButton() {
  int reading = digitalRead(BTN_D1);
  unsigned long now = millis();

  if (reading != lastButtonState) {
    lastDebounceTime = now;
  }

  if ((now - lastDebounceTime) > debounceDelay) {
    if (reading != stableButtonState) {
      stableButtonState = reading;

      if (stableButtonState == LOW) {
        state = (FaceState)((state + 1) % STATE_COUNT);
        stateJustChanged = true;
      }
    }
  }

  lastButtonState = reading;
}

// =====================================================
// SETUP
// =====================================================
void setup() {
  pinMode(BTN_D1, INPUT_PULLUP);
  u8g2.begin();
  randomSeed(analogRead(A0));
}

// =====================================================
// MAIN LOOP
// =====================================================
void loop() {
  handleButton();

  switch (state) {

    case STATE_IDLE:
      idle();
      break;

    case STATE_HAPPY:
      if (stateJustChanged) {
        playOnce(anim_happy);
        stateJustChanged = false;
      }
      break;

    case STATE_SAD:
      if (stateJustChanged) {
        playOnce(anim_sad);
        stateJustChanged = false;
      }
      break;

    case STATE_ANGRY:
      if (stateJustChanged) {
        playOnce(anim_angry);
        stateJustChanged = false;
      }
      break;

    case STATE_TIRED:
      if (stateJustChanged) {
        playOnce(anim_tired);
        stateJustChanged = false;
      }
      break;

    case STATE_HEART:
      if (stateJustChanged) {
        playOnce(anim_heart);
        stateJustChanged = false;
      }
      break;

    case STATE_SLEEPING: {
      static unsigned long lastAnimTime = 0;
      static uint8_t zState = 0;

      unsigned long now = millis();
      if (now - lastAnimTime > 500) {
        lastAnimTime = now;
        zState = (zState + 1) % 3;
      }

      u8g2.clearBuffer();
      u8g2.setFont(u8g2_font_6x12_tf);

      u8g2.drawStr(28, 32, "---");
      u8g2.drawStr(72, 32, "---");

      if (zState == 0) u8g2.drawStr(52, 16, "ZZZ");
      if (zState == 1) u8g2.drawStr(52, 16, "Zzz");
      if (zState == 2) u8g2.drawStr(52, 16, "zzZ");

      u8g2.sendBuffer();
      break;
    }
  }
}
